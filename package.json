{
  "name": "multi-tenant-app",
  "bin": {
    "analyze-code": "./src/bin/analyze-code.js"
  },
  "version": "1.0.0",
  "description": "Multi-tenant application with user management",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "build:sdk": "cd sdk && npm run build",
    "test:sdk": "cd sdk && npm test",
    "analyze": "ts-node src/bin/analyze-code.ts",
    "analyze:fix": "ts-node src/bin/analyze-code.ts --fix",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "bcrypt": "^5.x",
    "bull": "^4.x",
    "commander": "^8.3.0",
    "eslint": "^8.4.1",
    "express": "^4.x",
    "express-rate-limit": "^6.x",
    "express-validator": "^7.x",
    "glob": "^7.2.0",
    "helmet": "^8.0.0",
    "speakeasy": "^2.1.0",
    "qrcode": "^1.5.3",
    "jsonwebtoken": "^9.x",
    "multer": "^1.x",
    "nodemailer": "^6.x",
    "nodemailer-mailgun-transport": "^2.1.4",
    "handlebars": "^4.7.7",
    "crypto": "^1.0.1",
    "oauth2-server": "^3.x",
    "passport": "^0.6.x",
    "passport-google-oauth20": "^2.x",
    "passport-local": "^1.x",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.5.1",
    "rate-limit-redis": "^3.0.0",
    "redis": "^4.x",
    "redis-om": "^0.3.6",
    "sequelize": "^6.37.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.x",
    "@your-org/multitenant-sdk": "file:sdk"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.x",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.x",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^6.x",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.17",
    "@types/yamljs": "^0.2.33",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv": "^16.x",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.x",
    "jest-environment-node": "^29.x",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.x",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
